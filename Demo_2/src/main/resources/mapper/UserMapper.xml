<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lynu.chaohl.Demo2.mapper.UserMapper">

    <select id="queryAll" resultType="lynu.chaohl.Demo2.Entity.User">
        select * from userTest
    </select>


    <insert id="addUser"  parameterType="lynu.chaohl.Demo2.Entity.User">
        insert into userTest(username,id,password) values (#{username},#{id},#{password})
    </insert>
</mapper>
