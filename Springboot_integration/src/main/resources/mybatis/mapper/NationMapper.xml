<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.mapper.NationMapper">

  <!--复杂结果集映射-->
  <resultMap id="BaseResultMap" type="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.Nation">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="nationCode" jdbcType="VARCHAR" property="nationcode" />
    <result column="nationName" jdbcType="VARCHAR" property="nationname" />
  <!--关联对象映射-->
    <association property="province" javaType="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.Province">
      <id column="id" jdbcType="INTEGER" property="id" />
      <result column="provinceCode" jdbcType="VARCHAR" property="provincecode" />
      <result column="provinceNmae" jdbcType="VARCHAR" property="provincenmae" />
    </association>
    <!--集合映射-->
    <collection property="counties" ofType="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.County">
      <id column="id" jdbcType="INTEGER" property="id" />
      <result column="countyCode" jdbcType="VARCHAR" property="countycode" />
      <result column="countyName" jdbcType="VARCHAR" property="countyname" />
    </collection>
  </resultMap>


  <select id="getNations" resultMap="BaseResultMap">

    select a.id,a.nationCode,a.nationName,b.id,b.provinceCode,c.id
    from nation a left join province b on a.id=b.id left join county c on a.id=c.id
  </select>

  <sql id="Base_Column_List">
    id, nationCode, nationName
  </sql>
  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from nation
    where id = #{id,jdbcType=INTEGER}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from nation
    where id = #{id,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.Nation">
    insert into nation (id, nationCode, nationName
      )
    values (#{id,jdbcType=INTEGER}, #{nationcode,jdbcType=VARCHAR}, #{nationname,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.Nation">
    insert into nation
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="nationcode != null">
        nationCode,
      </if>
      <if test="nationname != null">
        nationName,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
      <if test="nationcode != null">
        #{nationcode,jdbcType=VARCHAR},
      </if>
      <if test="nationname != null">
        #{nationname,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.Nation">
    update nation
    <set>
      <if test="nationcode != null">
        nationCode = #{nationcode,jdbcType=VARCHAR},
      </if>
      <if test="nationname != null">
        nationName = #{nationname,jdbcType=VARCHAR},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  <update id="updateByPrimaryKey" parameterType="lynu.chaohl.SpringbootIntegration.ServerBaseDemo.entity.Nation">
    update nation
    set nationCode = #{nationcode,jdbcType=VARCHAR},
      nationName = #{nationname,jdbcType=VARCHAR}
    where id = #{id,jdbcType=INTEGER}
  </update>
</mapper>